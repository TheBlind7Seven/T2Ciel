#include "Calculatrice.h"

int main() {
	Calculatrice<int> c1(10, 5);
	c1.afficheResultat();

	Calculatrice<float> c2(7.5f, 2.5f);
	c2.afficheResultat();

	Calculatrice<double> c3(10.0, 3.0);
	c3.afficheResultat();
	return 0;
}
